{
  "name": "ayudante-virtual",
  "version": "0.0.1",
  "description": "Bot de Slack que sirve como ayudante virtual para realizar consultas que no pueden ser compartidas en canales públicos.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seblaz/Ayudante-virtual.git"
  },
  "keywords": [
    "slack",
    "ayudante",
    "trabajos",
    "prácticos"
  ],
  "author": "Seblaz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seblaz/Ayudante-virtual/issues"
  },
  "homepage": "https://github.com/seblaz/Ayudante-virtual#readme",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "test": "mocha -r should -r @babel/register -r should-sinon",
    "test:watch": "npm run test -- --watch",
    "build": "babel src -d dist",
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "npm run build && node dist/index.js"
  },
  "dependencies": {
    "@slack/bolt": "^1.8.0",
    "@slack/web-api": "latest",
    "exit-hook": "^2.2.0",
    "lowdb": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^9.0.1",
    "supertest": "^4.0.2"
  }
}
